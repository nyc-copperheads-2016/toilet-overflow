<h3><%= link_to "Post a new question", new_question_path %></h3>

<h1>Questions</h1>

<% @questions.each do |q| %>
  <h4><%= link_to q.question_title, question_path(q) %></h4>
  <p>Votes: <%= q.votes.all.sum(:vote) %></p>
  <p>Answers: <%= q.answers.count %></p>
  <p>Tags:<% q.tags.each do |t| %>
      <a href=""><%= t.name %></a>
  <% end %></p>
  <p>created on <%= q.created_at %> by <%= q.user.username %></p>
<% end %>

